<Window x:Class="Personas.NacionalidadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Personas"
        mc:Ignorable="d"
        ShowInTaskbar="False"
        ResizeMode="NoResize"
        Title="NacionalidadWindow"
        Height="115"
        Width="230">
    <StackPanel>
        <TextBlock>Nacionalidad</TextBlock>
        <TextBox></TextBox>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="Aceptar_Button" IsDefault="True" Click="Aceptar_Button_Click">Aceptar</Button>
            <Button IsCancel="True">Cancelar</Button>
        </StackPanel>
    </StackPanel>
</Window>
